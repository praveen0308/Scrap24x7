<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="500">
       <KeyFrameSet>
           <KeyAttribute
               motion:motionTarget="@+id/include2"
               motion:framePosition="0"
               android:alpha="0.85" />
           <KeyAttribute
               motion:motionTarget="@+id/include2"
               motion:framePosition="0"
               android:translationZ="6dp" />
       </KeyFrameSet>
        <OnClick motion:targetId="@+id/cl_toolbar_address" />
        <OnClick />
    </Transition>

    <ConstraintSet>
        <Constraint
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="match_parent"
            android:id="@+id/frameLayout6"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/cl_toolbar_address" />
        <Constraint
            motion:layout_constraintTop_toBottomOf="@+id/cl_toolbar_address"
            android:id="@+id/include3"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/layout_not_available"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_editor_absoluteX="156dp"
            motion:layout_constraintTop_toBottomOf="@+id/cl_toolbar_address"
            motion:layout_constraintBottom_toBottomOf="parent" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toBottomOf="@id/cl_toolbar_address"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/layout_not_available" />
        <Constraint
            android:id="@+id/layout_not_available"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toBottomOf="@id/cl_toolbar_address"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/layout_not_available"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toBottomOf="@id/cl_toolbar_address"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/layout_not_available"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toBottomOf="@id/cl_toolbar_address"
            motion:layout_constraintStart_toStartOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/include2"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
    </ConstraintSet>
</MotionScene>